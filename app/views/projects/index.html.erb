<%= link_to 'Add Project', new_project_path %><br>
<% @projects.each do |project| %>
  Project Name: <%= project.name %><br>
  Project Description: <%= project.description %><br>
  Project Lead: <%= project.lead.name %><br>
  <br>
  Project Members:
  <% project.members.each do |member| %>
    <%= member.name %><br>
  <% end %>
  <%= link_to "Join Team", "/projects/#{project.id}/join", method: :put %><br>
  <br>
<% end %>